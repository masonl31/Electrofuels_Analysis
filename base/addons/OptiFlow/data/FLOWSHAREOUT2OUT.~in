FLOWSHAREOUT2OUT(Y,IA,PROC,IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET)$(FLOWSHAREOUT2OUT('2014',IA,PROC,IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET))=FLOWSHAREOUT2OUT('2014',IA,PROC,IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET);

FLOWSHAREOUT2OUT(IA,'MSW_chp_large',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET)$(FLOWSHAREOUT2OUT('DK_CA_KBH','MSW_chp_large',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET) AND (PROCINOUTFLOW(IA,'MSW_chp_large',IFLOWIN,IFLOWOUT,'IONEMANY')))
=FLOWSHAREOUT2OUT('DK_CA_KBH','MSW_chp_large',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET);

FLOWSHAREOUT2OUT(IA,'MSW_chp_medium',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET)$(FLOWSHAREOUT2OUT('DK_CA_Kal','MSW_chp_medium',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET) AND (PROCINOUTFLOW(IA,'MSW_chp_medium',IFLOWIN,IFLOWOUT,'IONEMANY')))
=FLOWSHAREOUT2OUT('DK_CA_Kal','MSW_chp_medium',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET) ;

FLOWSHAREOUT2OUT(IA,'MSW_chp_small',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET)$(FLOWSHAREOUT2OUT('DK_MAM_Slagelse','MSW_chp_small',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET) AND (PROCINOUTFLOW(IA,'MSW_chp_small',IFLOWIN,IFLOWOUT,'IONEMANY')))
=FLOWSHAREOUT2OUT('DK_MAM_Slagelse','MSW_chp_small',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET);

FLOWSHAREOUT2OUT(IA,'MSW_chp_verysmall',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET)$(FLOWSHAREOUT2OUT('DK_E_DTU','MSW_chp_verysmall',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET) AND (PROCINOUTFLOW(IA,'MSW_chp_verysmall',IFLOWIN,IFLOWOUT,'IONEMANY')))
=FLOWSHAREOUT2OUT('DK_E_DTU','MSW_chp_verysmall',IFLOWIN,IFLOWOUT,IFLOWOUT2,ILOUPFXSET);
