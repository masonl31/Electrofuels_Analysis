IDH2_T_Y(IR,S,T) =
    ((DH2(Y,IR) *DH2_VAR_T(IR,S,T)) / IDH2_SUMST(IR))$(IDH2_SUMST(IR) GT 0);

display IDH2_T_Y;

* Transfer start level in hydrogen storage into internal parameter
*$ifi %bb3%==yes  loop((IA,IGH2STO)$IAGK_Y(IA,IGH2STO),IH2STOVOL_START(IA,IGH2STO,S)= H2STOVOL_START(Y,IA,IGH2STO,S));
*$ifi %bb3%==yes  loop((IA,IGH2STO)$IAGK_Y(IA,IGH2STO),IH2STOVOL_END(IA,IGH2STO,S)= IH2STOVOL_START(IA,IGH2STO,S+1));

$ifi %bb3%==yes  IH2STOVOL_START(IA,IGH2STO,SSS)= H2STOVOL_START(Y,IA,IGH2STO,SSS);
$ifi %bb3%==yes  IH2STOVOL_END(IA,IGH2STO,SSS)= IH2STOVOL_START(IA,IGH2STO,SSS++1);

$ifi %bb3%==yes display IH2STOVOL_START,IH2STOVOL_END,S;
