FUELPRICE(Y,IA,'NAT_GAS')$(ICA('FINLAND',IA) AND (FUELPRICE(Y,IA,'NAT_GAS') EQ 0))=FUELPRICE(Y,'FI_R_Urban','NAT_GAS');
FUELPRICE(Y,IA,'NAT_GAS')$(ICA('SWEDEN',IA) AND (FUELPRICE(Y,IA,'NAT_GAS') EQ 0))=1.1*FUELPRICE(Y,'SE_M_Urban','NAT_GAS');

FUELPRICE(Y,IA,'NG_CCS')$(FUELPRICE(Y,IA,'NAT_GAS'))=FUELPRICE(Y,IA,'NAT_GAS');
FUELPRICE(Y,IA,'LIGNITE')$(FUELPRICE(Y,IA,'COAL'))=0.5*FUELPRICE(Y,IA,'COAL');
FUELPRICE(Y,IA,'WP_CCS')$(FUELPRICE(Y,IA,'WOOD_PELLETS'))=FUELPRICE(Y,IA,'WOOD_PELLETS');
FUELPRICE(Y,IA,'COAL_CCS')$(FUELPRICE(Y,IA,'COAL'))=FUELPRICE(Y,IA,'COAL');

FUELPRICE(Y,IA,'LIGHTOIL')$((NOT (ICA('DENMARK',IA))) AND (FUELPRICE(Y,IA,'LIGHTOIL') EQ 0))=FUELPRICE(Y,'NO_M_Rural','LIGHTOIL');

