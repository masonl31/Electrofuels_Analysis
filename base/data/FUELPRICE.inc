FUELPRICE(Y,IA,'NAT_GAS')$(ICA('NORWAY',IA) AND (FUELPRICE(Y,IA,'NAT_GAS') EQ 0))=FUELPRICE(Y,'NO_M_Rural','NAT_GAS');
